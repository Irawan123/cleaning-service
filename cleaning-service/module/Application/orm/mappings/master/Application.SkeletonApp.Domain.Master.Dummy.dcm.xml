<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          http://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Application\SkeletonApp\Domain\Master\Dummy" table="master_dummy">

        <id name="id" type="integer" column="id">
            <generator strategy="AUTO" />
            <sequence-generator sequence-name="dummy_seq" allocation-size="1" initial-value="1" />

        </id>

        <field name="nama" column="nama" type="string" length="100" unique="true" />
        <field name="jabatan" column="jabatan" type="text" nullable="true" />
        <field name="foto" column="foto" type="text" nullable="true" />

    </entity>
</doctrine-mapping>