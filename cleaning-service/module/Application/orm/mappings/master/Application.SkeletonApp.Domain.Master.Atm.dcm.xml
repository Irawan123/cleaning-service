<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          http://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Application\SkeletonApp\Domain\Master\Atm" table="atm">

        <id name="id" type="integer" column="id">
            <generator strategy="AUTO" />
            <sequence-generator sequence-name="atm_seq" allocation-size="1" initial-value="1" />
        </id>

        <field name="idAtm" column="id_atm" type="text" />
        <field name="jenisAtm" column="jenis_atm" type="text" nullable="false" />

		<many-to-one target-entity="Application\SkeletonApp\Domain\Master\Lokasi" field="lokasi" inversed-by="atm" />		
		
    </entity>
</doctrine-mapping>