<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          http://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Application\SkeletonApp\Domain\Master\FotoDetail" table="foto_detail">

        <id name="id" type="integer" column="id">
            <generator strategy="AUTO" />
            <sequence-generator sequence-name="foto_detail_seq" allocation-size="1" initial-value="1" />
        </id>

        <field name="keterangan" column="keterangan" type="text" nullable="true" />
        <field name="foto" column="foto" type="text" nullable="false" />

		<many-to-one target-entity="Application\SkeletonApp\Domain\Master\Lokasi" field="lokasi" inversed-by="foto_detail" />

		<many-to-one target-entity="Application\SkeletonApp\Domain\Master\ListFotoDetail" field="list_foto_detail" inversed-by="foto_detail" />
		
    </entity>
</doctrine-mapping>