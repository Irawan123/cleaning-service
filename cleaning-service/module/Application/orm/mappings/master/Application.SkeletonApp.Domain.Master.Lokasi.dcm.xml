<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          http://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Application\SkeletonApp\Domain\Master\Lokasi" table="lokasi">

        <id name="id" type="integer" column="id">
            <generator strategy="AUTO" />
            <sequence-generator sequence-name="lokasi_seq" allocation-size="1" initial-value="1" />

        </id>

        <field name="nama" column="nama" type="text" nullable="false" />
        <field name="alamat" column="alamat" type="text" nullable="false" />
        <field name="jumlahAtm" column="jumlah_atm" type="integer" nullable="true" />
        <field name="keterangan" column="keterangan" type="text" nullable="true" />
		
		<one-to-many target-entity="Application\SkeletonApp\Domain\Master\Atm" field="atm" mapped-by="lokasi" >
        </one-to-many>

		<one-to-many target-entity="Application\SkeletonApp\Domain\Master\FotoDetail" field="foto_detail" mapped-by="lokasi" >
        </one-to-many>

		<one-to-many target-entity="Application\SkeletonApp\Domain\Master\FotoSebelum" field="foto_sebelum" mapped-by="lokasi" >
        </one-to-many>

		<one-to-many target-entity="Application\SkeletonApp\Domain\Master\FotoSesudah" field="foto_sesudah" mapped-by="lokasi" >
        </one-to-many>

		<one-to-many target-entity="Application\SkeletonApp\Domain\Master\FotoTemuan" field="foto_temuan" mapped-by="lokasi" >
        </one-to-many>		

		<many-to-one target-entity="Application\SkeletonApp\Domain\Master\Zona" field="zona" inversed-by="lokasi" />		
		
    </entity>
</doctrine-mapping>