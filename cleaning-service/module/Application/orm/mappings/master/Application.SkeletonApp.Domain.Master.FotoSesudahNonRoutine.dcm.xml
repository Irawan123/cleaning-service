<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          http://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Application\SkeletonApp\Domain\Master\FotoSesudahNonRoutine" table="foto_sesudah_non_routine">

        <id name="id" type="integer" column="id">
            <generator strategy="AUTO" />
            <sequence-generator sequence-name="foto_sesudah_non_routine_seq" allocation-size="1" initial-value="1" />

        </id>

        <field name="lokasi" column="lokasi" type="text" nullable="false" />
        <field name="alamat" column="alamat" type="text" nullable="false" />
        <field name="keterangan" column="keterangan" type="text" nullable="true" />
        <field name="foto" column="foto" type="text" nullable="false" />

		<many-to-one target-entity="Application\SkeletonApp\Domain\Master\Pegawai" field="pegawai" inversed-by="foto_sesudah_non_routine" />		
		
    </entity>
</doctrine-mapping>