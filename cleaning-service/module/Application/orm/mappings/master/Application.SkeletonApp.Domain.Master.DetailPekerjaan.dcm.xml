<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          http://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Application\SkeletonApp\Domain\Master\DetailPekerjaan" table="detail_pekerjaan">

        <id name="id" type="integer" column="id">
            <generator strategy="AUTO" />
            <sequence-generator sequence-name="detail_pekerjaan_seq" allocation-size="1" initial-value="1" />
        </id>
		
		<many-to-one target-entity="Application\SkeletonApp\Domain\Master\Pegawai" field="pegawai" inversed-by="detail_pekerjaan" />

		<many-to-one target-entity="Application\SkeletonApp\Domain\Master\Pekerjaan" field="pekerjaan" inversed-by="detail_pekerjaan" />		
		
    </entity>
</doctrine-mapping>