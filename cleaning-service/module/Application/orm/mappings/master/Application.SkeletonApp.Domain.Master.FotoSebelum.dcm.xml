<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          http://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Application\SkeletonApp\Domain\Master\FotoSebelum" table="foto_sebelum">

        <id name="id" type="integer" column="id">
            <generator strategy="AUTO" />
            <sequence-generator sequence-name="foto_sebelum_seq" allocation-size="1" initial-value="1" />
        </id>

        <field name="tanggal" column="tanggal" type="datetime" nullable="false" />
        <field name="foto" column="foto" type="text" nullable="false" />

		<many-to-one target-entity="Application\SkeletonApp\Domain\Master\Lokasi" field="lokasi" inversed-by="foto_sebelum" />

		<many-to-one target-entity="Application\SkeletonApp\Domain\Master\Pegawai" field="pegawai" inversed-by="foto_sebelum" />		
		
    </entity>
</doctrine-mapping>